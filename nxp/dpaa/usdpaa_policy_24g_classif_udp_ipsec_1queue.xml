	</distribution>

<!--	xxxxxxxxxxxxxxxxxxxxxxxxxx Policies xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx -->

	<policy name="fm_policy_3">
		<dist_order>
			<distributionref name="dpdk_mac3_dist"/>
			<distributionref name="default_mac3_dist"/>
		</dist_order>
	</policy>

	<policy name="fm_policy_4">
		<dist_order>
